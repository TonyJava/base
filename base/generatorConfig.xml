        <!--生成映射文件存放位置-->
        <sqlMapGenerator targetPackage="com.hzlh.mapping" targetProject="base">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!--生成Dao类存放位置-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.hzlh.dao" targetProject="base">
